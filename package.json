        "build": "rm -rf ./dist && npm run build:vanilla && npm run build:vue",
        "build:cdn": "sh build-cdn.sh",
        "build:vanilla": "sh build-vanilla.sh",
        "build:vue": "sh build-vue.sh",
        "serve:vanilla": "./node_modules/.bin/webpack serve --config webpack-vanilla.config.js --mode=development",
        "serve:vue": "cd sandbox/vue && npm run serve",
